# Options related to all Physics
[Physics]

enabled_physics = 'LowMachNavierStokes'

# Options for Incompressible Navier-Stokes physics
[./LowMachNavierStokes]

V_FE_family = 'LAGRANGE'
P_FE_family = 'LAGRANGE'
T_FE_family = 'LAGRANGE'

V_order = 'SECOND'
T_order = 'SECOND'
P_order = 'FIRST'

# Thermodynamic pressure
p0 = '1' #[Pa]

# Reference temperature
T0 = '1' #[K]

# Gas constant
R = '1.0' #[J/kg-K]

# Gravity vector
g = '0 -9.8 0' #[m/s^2]

conductivity_model = 'constant'
viscosity_model = 'constant'
specific_heat_model = 'constant'

# Boundary ids:
# k = bottom -> 0
# k = top    -> 5
# j = bottom -> 1
# j = top    -> 3
# i = bottom -> 4
# i = top    -> 2

vel_bc_ids = '0 1 2 3 4 5'
vel_bc_types = 'no_slip no_slip no_slip no_slip no_slip no_slip'


enable_thermo_press_calc = 'false'

pin_pressure = true
pin_location = '0.0 0.01'
pin_value = '0.0'

output_vars = 'rho'

[../VariableNames]

Temperature = 'T'
u_velocity = 'u'
v_velocity = 'v'                                                                                                                                                                                                                              
w_velocity = 'w'
pressure = 'p'

[]


[QoI]

enabled_qois = 'average_nusselt_number'

[./NusseltNumber]

thermal_conductivity = '0.041801' #[W/m-K]

bc_ids = '3'

scaling = '0.0332262120257623' #'${/ ${Pr_over_TwomuT} ${Physics/LowMachNavierStokes/epsilon} }'

[]



[Materials]

[./Viscosity]

mu = '1.0' #[kg/m-s]

[../Conductivity]

k = '1.0' #[W/m-K]

[../SpecificHeat]

cp = '1.0' #[J/kg-K]

[]



# Mesh related options
[Mesh]
   [./Generation]
      dimension = '3'
      element_type = 'HEX27'
      n_elems_x = '3'
      n_elems_y = '3'
      n_elems_z = '3'
[]

# Options for tiem solvers
[unsteady-solver]
transient = false 

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 100 
max_linear_iterations = 2500

verify_analytic_jacobians = 5.0e-6

initial_linear_tolerance = 1.0e-10

use_numerical_jacobians_only = 'false'

# Visualization options
[vis-options]
output_vis = 'true'

vis_output_file_prefix = '3d_low_mach_jacobians_xz' 

output_residual = 'false' 

output_format = 'ExodusII'

# Options for print info to the screen
[screen-options]

system_name = 'LowMachNavierStokes'

print_equation_system_info = true
print_mesh_info = true
print_log_info = true
solver_verbose = true
solver_quiet = false


print_qoi = 'true'
echo_qoi = 'true'

print_element_jacobians = 'false'

[]
