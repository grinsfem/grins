# Options related to all Physics
[Physics]

enabled_physics = 'LowMachNavierStokes'

# Options for Incompressible Navier-Stokes physics
[./LowMachNavierStokes]

material = 'TestMaterial'

# Gravity vector
g = '0 -9.8 0' #[m/s^2]

enable_thermo_press_calc = 'false'

pin_pressure = true
pin_location = '0.0 0.01'
pin_value = '0.0'

output_vars = 'rho'

[]

[BoundaryConditions]
   bc_ids = '0:1:2:3:4:5'
   bc_id_name_map = 'AllWalls'

   [./AllWalls]
      [./Velocity]
         type = 'no_slip'
      [../]

      [./Temperature]
         type = 'homogeneous_neumann'
      [../]
[]

[Variables]
   [./Velocity]
      names = 'u v w'
      fe_family = 'LAGRANGE'
      order = 'SECOND'

   [../Pressure]
      names = 'p'
      fe_family = 'LAGRANGE'
      order = 'FIRST'

   [../Temperature]
      names = 'T'
      fe_family = 'LAGRANGE'
      order = 'SECOND'
[]

[QoI]

enabled_qois = 'average_nusselt_number'

adjoint_sensitivity_parameters = 'Materials/TestMaterial/ThermodynamicPressure/value Materials/TestMaterial/ReferenceTemperature/value Materials/TestMaterial/GasConstant/value Materials/TestMaterial/Viscosity/value Materials/TestMaterial/ThermalConductivity/value Materials/TestMaterial/SpecificHeat/value QoI/NusseltNumber/thermal_conductivity'

[./NusseltNumber]

thermal_conductivity = '0.041801' #[W/m-K]

bc_ids = '3'

scaling = '0.0332262120257623' #'${/ ${Pr_over_TwomuT} ${Physics/LowMachNavierStokes/epsilon} }'

[]



[Materials]
  [./TestMaterial]
    [./Viscosity]
       model = 'constant'
       value = '1.0' #[kg/m-s]
    [../ThermalConductivity]
       model = 'constant'
       value = '1.0' #[W/m-K]
    [../SpecificHeat]
       model = 'constant'
       value = '1.0' #[J/kg-K]
    [../ReferenceTemperature]
       value = '1.0'
    [../ThermodynamicPressure]
       value = '1.0'
    [../GasConstant]
       value = '1.0'
[]



# Mesh related options
[Mesh]
   [./Generation]
      dimension = '3'
      element_type = 'HEX27'
      n_elems_x = '3'
      n_elems_y = '3'
      n_elems_z = '3'
[]

# Options for time solvers



#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 100
max_linear_iterations = 2500

verify_analytic_jacobians = 5.0e-6

initial_linear_tolerance = 1.0e-10

use_numerical_jacobians_only = 'false'

# Visualization options
[vis-options]
output_vis = 'false'

vis_output_file_prefix = '3d_low_mach_jacobians_xy'

output_residual = 'false'

output_format = 'ExodusII'

[Output]
   [./Display]
      print_qoi = 'true'
   [../]
[]

# Options for print info to the screen
[screen-options]

system_name = 'LowMachNavierStokes'

print_equation_system_info = true
print_mesh_info = true
print_log_info = true
solver_verbose = true
solver_quiet = false

echo_qoi = 'true'

print_element_jacobians = 'false'

[]
