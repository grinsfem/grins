# Mesh related options
[Mesh]
   [./Generation]
      dimension = '2'
      element_type = 'QUAD9'
      n_elems_x = '10'
      n_elems_y = '10'
      x_max = '5.0'
[]

[linear-nonlinear-solver]
   max_nonlinear_iterations = 10
   max_linear_iterations = 2500
   relative_residual_tolerance = 1.0e-11
   relative_step_tolerance = 1.0e-11
   verify_analytic_jacobians = 1.e-6
[]

[Materials]
   [./TestMaterial]
      [./Viscosity]
          model = 'parsed'
          value = '1.0'

      [../ThermalConductivity]
          model = 'parsed'
          value = '1.0'
    [../Density]
      value = '1.0'
    [../SpecificHeat]
      model = 'constant'
      value = '1.0'
[]

[vis-options]
   output_vis = 'true'
   vis_output_file_prefix = 'stokes_poiseuille_flow_parsed_viscosity_parsed_conductivity'
   output_format = 'xdr'
[]

# Options for print info to the screen
[screen-options]
   print_equation_system_info = 'true'
   print_mesh_info = 'true'
   print_log_info = 'true'
   solver_verbose = 'true'
   solver_quiet = 'false'
   echo_physics = 'true'
   system_name = 'GRINS-TEST'
[]

[Physics]

   enabled_physics = 'Stokes HeatConduction'

   [./Stokes]

      material = 'TestMaterial'

      bc_ids = '1 3 1 3 2 0'
      bc_types = 'parsed_dirichlet parsed_dirichlet parsed_dirichlet parsed_dirichlet no_slip no_slip'
      bc_variables = 'u u v v na na'
      bc_values = '{4*y*(1-y)} {4*y*(1-y)} 0.0 0.0 na na'

      pin_pressure = true
      pin_value = 100.0
      pin_location = '2.5 0.5'

   [../HeatConduction]

       material = 'TestMaterial'

       bc_ids = '3 1'
       bc_types = 'isothermal_wall isothermal_wall'

       T_wall_1 = 1
       T_wall_3 = 10
[]

[Variables]
   [./Velocity]
      names = 'u v'
      fe_family = 'LAGRANGE'
      order = 'SECOND'
   [../Pressure]
      names = 'p'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
   [../Temperature]
      names = 'T'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]
