# Mesh related options
[Mesh]
   [./Read]
      filename = '@abs_top_srcdir@/test/grids/coupled_stokes_ns.e'
[]

#uniformly_refine = '2'

# Options for time solvers
[unsteady-solver]
transient = false 

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 20
max_linear_iterations = 2500

minimum_linear_tolerance = '1e-6'
relative_step_tolerance = '1.0e-11'

verify_analytic_jacobians = '1.e-6'

# Visualization options
[vis-options]
output_vis = 'false' 
vis_output_file_prefix = 'stokes_ns'
output_format = 'ExodusII'

# Options for print info to the screen
[screen-options]
print_equation_system_info = 'true' 
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

echo_physics = 'true'

[]

# Materials
[Materials]
   [./TestMaterial]
      [./Viscosity]
         model = 'constant'
         value = '1.0e-3'
      [../Density]
         value = '1.0'
[]


# Options related to all Physics
[Physics]

enabled_physics = 'IncompressibleNavierStokes Stokes'

# Options for Stokes physics
[./Stokes]

material = 'TestMaterial'

enabled_subdomains = '2'

FE_family = LAGRANGE
V_order = SECOND
P_order = FIRST

bc_ids = '4 6 7 8'
bc_types = 'no_slip no_slip parabolic_profile no_slip'

parabolic_profile_coeffs_7 = '0.0 0.0 -4.0 0.0 4.0 0.0'
parabolic_profile_var_7 = 'u'
parabolic_profile_fix_7 = 'v'

# Options for Navier Stokes physics
[../IncompressibleNavierStokes]

material = 'TestMaterial'

enabled_subdomains = '1'

FE_family = LAGRANGE
V_order = SECOND
P_order = FIRST

bc_ids = '1 2 3'
bc_types = 'no_slip no_slip no_slip'

[../VariableNames]

u_velocity = 'u'
v_velocity = 'v'
pressure = 'p'

[]
